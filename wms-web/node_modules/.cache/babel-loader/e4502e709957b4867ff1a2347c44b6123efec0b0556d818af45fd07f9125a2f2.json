{"ast":null,"code":"// import axios from \"axios\";axios\n\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Main',\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      name: '',\n      sex: '',\n      sexs: [{\n        value: 0,\n        label: \"女\"\n      }, {\n        value: 1,\n        label: \"男\"\n      }, {\n        value: 3,\n        label: \"请选择\"\n      }],\n      centerDialogVisible: false,\n      form: {\n        name: '',\n        no: '',\n        password: '',\n        age: '',\n        phone: '',\n        sex: '',\n        roleId: '1'\n      },\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入名字',\n          trigger: 'blur'\n        }],\n        no: [{\n          required: true,\n          message: '请输入账号',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 8,\n          message: '长度在 3 到 8 个字符',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 8,\n          message: '长度在 3 到 8 个字符',\n          trigger: 'blur'\n        }]\n      },\n      loading: true\n    };\n  },\n  beforeMount() {\n    // this.loadGet();\n    // this.loadPost();\n  },\n  methods: {\n    loadGet() {\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\n        console.log(res);\n      });\n    },\n    loadPost() {\n      if (this.sex === 3) {\n        this.sex = '';\n      }\n      this.$axios.post(this.$httpUrl + '/user/listPage1', {\n        param: {\n          name: this.name,\n          sex: this.sex\n        },\n        pageNum: this.pageNum,\n        pageSize: this.pageSize\n      }).then(res => res.data).then(res => {\n        console.log(res.data);\n        if (res.code === 200) {\n          //判断状态码是否200\n          //结果集的数据传入tableData\n          this.tableData = res.data;\n          this.total = res.total;\n        } else {\n          alert(\"查询失败\" + res.code);\n        }\n        // this.tableData=res.data\n        // console.log(res)\n      });\n    },\n\n    search() {\n      this.pageNum = 1;\n      this.loadPost();\n    },\n    reSet() {\n      this.name = '';\n      this.sex = '';\n    },\n    add() {\n      this.centerDialogVisible = true;\n    },\n    save() {\n      this.$axios.post(this.$httpUrl + '/user/save', this.form).then(res => res.data).then(res => {\n        console.log(res.data);\n        if (res.code === 200) {\n          //判断状态码是否200\n          this.$message({\n            message: '新增用户成功~~~',\n            type: 'success'\n          });\n          this.loadPost();\n        } else {\n          this.$message({\n            message: '新增用户失败！！！',\n            type: 'error'\n          });\n        }\n      });\n    },\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageSize = val;\n      this.pageNum = 1;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    }\n  }\n};","map":{"version":3,"mappings":"AAoHA;;AAEA;EAEA;EACAA;EACAC;IAEA;MACAC;MACAC;MACAC;MACAC;MACAL;MACAM;MACAC,OACA;QACAC;QACAC;MACA,GACA;QACAD;QACAC;MACA,GACA;QACAD;QACAC;MACA,EACA;MACAC;MACAC;QACAX;QACAY;QACAC;QACAC;QACAC;QACAT;QACAU;MACA;MACAC;QACAjB,OACA;UAAAkB;UAAAC;UAAAC;QAAA,EACA;QACAR,KACA;UAAAM;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,EACA;QACAP,WACA;UAAAK;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA;MAEA;MACAG;IAEA;EACA;EACAC;IACA;IACA;EAAA,CACA;EACAC;IACAC;MACA;QACAC;MACA;IACA;IACAC;MACA;QACA;MACA;MACA;QACAC;UACA7B;UACAM;QACA;QACAH;QACAC;MACA;QACAuB;QACA;UAAA;UACA;UACA;UACA;QACA;UACAG;QACA;QACA;QACA;MACA;IACA;;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;QACAP;QACA;UAAA;UACA;YACAR;YACAgB;UACA;UACA;QACA;UACA;YACAhB;YACAgB;UACA;QACA;MACA;IACA;IACAC;MACAT;MACA;MACA;MACA;IACA;IACAU;MACAV;MACA;MACA;IACA;EACA;AAEA","names":["name","data","tableData","pageNum","pageSize","total","sex","sexs","value","label","centerDialogVisible","form","no","password","age","phone","roleId","rules","required","message","trigger","min","max","loading","beforeMount","methods","loadGet","console","loadPost","param","alert","search","reSet","add","save","type","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/components","sources":["Main.vue"],"sourcesContent":["\r\n<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 5px\">\r\n      <span style=\"margin-left: 5px;font-size: 14px;color: #3f3f3f\">姓名：</span>\r\n      <el-input placeholder=\"请输入名字\"\r\n                v-model=\"name\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 160px\"\r\n                @keyup.enter.native=\"search\">\r\n      </el-input>\r\n      <span style=\"margin-left: 20px;font-size: 14px;color: #3f3f3f\">性别：</span>\r\n      <el-select v-model=\"sex\" filterable placeholder=\"请选择性别\" style=\"margin-left: 5px\">\r\n        <el-option\r\n            v-for=\"item in sexs\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\">\r\n        </el-option>\r\n      </el-select>\r\n\r\n      <el-button style=\"margin-left: 5px\" round @click=\"search\" type=\"primary\">查询</el-button>\r\n      <el-button type=\"success\" round @click=\"reSet\">重置</el-button>\r\n      <el-button type=\"success\" round @click=\"add\">新增</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\"\r\n              :header-cell-style=\"{background:'#d7d7d7',color:'#564d4d'}\"\r\n              border>\r\n      <el-table-column prop=\"id\" label=\"id\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"no\" label=\"账号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"sex\" label=\"性别\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.sex === 1 ? 'primary' : 'danger'\"\r\n              disable-transitions>{{ scope.row.sex === 1 ? '男' : '女' }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"age\" label=\"年龄\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"roleId\" label=\"角色\" width=\"140 \">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.roleId === 0 ? 'danger' : (scope.row.roleId === 1 ? 'success' : 'primary')\"\r\n              disable-transitions>{{ scope.row.roleId === 0 ? '超级管理员' : (scope.row.roleId === 1 ? '管理员' : '用户') }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"operation\" label=\"操作\">\r\n        <el-button type=\"success\" round>编辑</el-button>\r\n        <el-button type=\"danger\" round>删除</el-button>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[10,20,50, 100, 200]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n    <el-dialog\r\n        title=\"提示\"\r\n        :visible.sync=\"centerDialogVisible\"\r\n        width=\"30%\"\r\n        center>\r\n      <el-form ref=\"form\" :model=\"form\" rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"姓名\" prop=\"name\">\r\n          <el-col :span=\"15\">\r\n            <el-input v-model=\"form.name\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"账号\">\r\n          <el-col :span=\"15\">\r\n            <el-input v-model=\"form.no\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-col :span=\"15\">\r\n            <el-input v-model=\"form.password\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄\">\r\n          <el-col :span=\"15\">\r\n            <el-input v-model=\"form.age\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\">\r\n          <el-col :span=\"15\">\r\n            <el-input v-model=\"form.phone\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio-group v-model=\"form.sex\">\r\n            <el-radio label=\"1\">男</el-radio>\r\n            <el-radio label=\"0\">女</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </span>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\n\r\n// import axios from \"axios\";axios\r\n\r\nexport default {\r\n\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Main',\r\n  data() {\r\n\r\n    return {\r\n      tableData: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      name: '',\r\n      sex: '',\r\n      sexs: [\r\n        {\r\n          value: 0,\r\n          label: \"女\",\r\n        },\r\n        {\r\n          value: 1,\r\n          label: \"男\"\r\n        },\r\n        {\r\n          value: 3,\r\n          label: \"请选择\"\r\n        }\r\n      ],\r\n      centerDialogVisible: false,\r\n      form: {\r\n        name: '',\r\n        no: '',\r\n        password: '',\r\n        age: '',\r\n        phone: '',\r\n        sex: '',\r\n        roleId: '1'\r\n      },\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入名字', trigger: 'blur'},\r\n        ],\r\n        no: [\r\n          {required: true, message: '请输入账号', trigger: 'blur'},\r\n          {min: 3, max: 8, message: '长度在 3 到 8 个字符', trigger: 'blur'}\r\n        ],\r\n        password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n          {min: 3, max: 8, message: '长度在 3 到 8 个字符', trigger: 'blur'}\r\n        ],\r\n      },\r\n      loading: true\r\n\r\n    }\r\n  },\r\n  beforeMount() {\r\n    // this.loadGet();\r\n    // this.loadPost();\r\n  },\r\n  methods: {\r\n    loadGet() {\r\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\r\n        console.log(res)\r\n      })\r\n    },\r\n    loadPost() {\r\n      if (this.sex === 3) {\r\n        this.sex = ''\r\n      }\r\n      this.$axios.post(this.$httpUrl + '/user/listPage1', {\r\n        param: {\r\n          name: this.name,\r\n          sex: this.sex\r\n        },\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize\r\n      }).then(res => res.data).then(res => {\r\n        console.log(res.data)\r\n        if (res.code === 200) {   //判断状态码是否200\r\n          //结果集的数据传入tableData\r\n          this.tableData = res.data\r\n          this.total = res.total\r\n        } else {\r\n          alert(\"查询失败\" + res.code)\r\n        }\r\n        // this.tableData=res.data\r\n        // console.log(res)\r\n      })\r\n    },\r\n    search() {\r\n      this.pageNum = 1;\r\n      this.loadPost()\r\n    },\r\n    reSet() {\r\n      this.name = '';\r\n      this.sex = '';\r\n    },\r\n    add() {\r\n      this.centerDialogVisible = true\r\n    },\r\n    save() {\r\n      this.$axios.post(this.$httpUrl + '/user/save', this.form).then(res => res.data).then(res => {\r\n        console.log(res.data)\r\n        if (res.code === 200) {   //判断状态码是否200\r\n          this.$message({\r\n            message: '新增用户成功~~~',\r\n            type: 'success'\r\n          })\r\n          this.loadPost()\r\n        } else {\r\n          this.$message({\r\n            message: '新增用户失败！！！',\r\n            type: 'error'\r\n          });\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageSize = val;\r\n      this.pageNum = 1;\r\n      this.loadPost()\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum = val;\r\n      this.loadPost()\r\n    }\r\n  },\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n<style>\r\nbody {\r\n  margin: 0;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}