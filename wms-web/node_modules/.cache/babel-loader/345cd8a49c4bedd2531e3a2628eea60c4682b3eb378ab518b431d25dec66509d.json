{"ast":null,"code":"// import axios from \"axios\";axios\n\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Main',\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 20,\n      total: 0\n    };\n  },\n  beforeMount() {\n    // this.loadGet();\n    this.loadPost();\n  },\n  methods: {\n    loadGet() {\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\n        console.log(res);\n      });\n    },\n    loadPost() {\n      this.$axios.post(this.$httpUrl + '/user/listPage1', {\n        param: {\n          name: \"\"\n        },\n        pageNum: this.pageNum,\n        pageSize: this.pageSize\n      }).then(res => res.data).then(res => {\n        console.log(res.data);\n        if (res.code === 200) {\n          //判断状态码是否200\n          //结果集的数据传入tableData\n          this.tableData = res.data;\n          this.total = res.total;\n        } else {\n          alert(\"查询失败\" + res.code);\n        }\n        // this.tableData=res.data\n        // console.log(res)\n      });\n    },\n\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageSize = val;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    }\n  }\n};","map":{"version":3,"mappings":"AA+CA;;AAEA;EAEA;EACAA;EACAC;IAEA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;EACAC;IACAC;MACA;QACAC;MACA;IACA;IACAC;MACA;QACAC;UAAAX;QAAA;QACAG;QACAC;MACA;QACAK;QACA;UAAA;UACA;UACA;UACA;QACA;UACAG;QACA;QACA;QACA;MACA;IACA;;IACAC;MACAJ;MACA;MACA;IACA;IACAK;MACAL;MACA;MACA;IACA;EACA;AAGA","names":["name","data","tableData","pageNum","pageSize","total","beforeMount","methods","loadGet","console","loadPost","param","alert","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/components","sources":["Main.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-table :data=\"tableData\"\r\n              :header-cell-style=\"{background:'#d7d7d7',color:'#564d4d'}\"\r\n              border\r\n    >\r\n      <el-table-column prop=\"id\" label=\"id\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"no\" label=\"账号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"sex\" label=\"性别\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.sex === 1 ? 'primary' : 'danger'\"\r\n              disable-transitions>{{ scope.row.sex === 1 ? '男' : '女' }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"age\" label=\"年龄\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"roleId\" label=\"角色\" width=\"140 \">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.roleId === 0 ? 'danger' : (scope.row.roleId === 1 ? 'success' : 'primary')\"\r\n              disable-transitions>{{ scope.row.roleId === 0 ? '超级管理员' : (scope.row.roleId === 1 ? '管理员' : '用户') }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"operation\" label=\"操作\">\r\n        <el-button type=\"success>编辑</el-button>\r\n        <el-button>删除</el-button>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[20,50, 100, 200]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n// import axios from \"axios\";axios\r\n\r\nexport default {\r\n\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Main',\r\n  data() {\r\n\r\n    return {\r\n      tableData: [],\r\n      pageNum: 1,\r\n      pageSize: 20,\r\n      total: 0,\r\n    }\r\n  },\r\n  beforeMount() {\r\n    // this.loadGet();\r\n    this.loadPost();\r\n  },\r\n  methods: {\r\n    loadGet() {\r\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\r\n        console.log(res)\r\n      })\r\n    },\r\n    loadPost() {\r\n      this.$axios.post(this.$httpUrl + '/user/listPage1', {\r\n        param: {name: \"\"},\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize\r\n      }).then(res => res.data).then(res => {\r\n        console.log(res.data)\r\n        if (res.code === 200) {   //判断状态码是否200\r\n          //结果集的数据传入tableData\r\n          this.tableData = res.data\r\n          this.total=res.total\r\n        } else {\r\n          alert(\"查询失败\" + res.code)\r\n        }\r\n        // this.tableData=res.data\r\n        // console.log(res)\r\n      })\r\n    },\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageSize=val;\r\n      this.loadPost()\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum=val;\r\n      this.loadPost()\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}